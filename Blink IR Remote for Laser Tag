//****************************************************************************
// Sets Critical
BLYNK_WRITE(V4){
  int i=param.asInt();
  if (i==1){C[0]=1050;}
  else {C[0]=525;}
  paritycheck();
}
//****************************************************************************
// Sets Power
BLYNK_WRITE(V5){
  int i=param.asInt();
  if (i==0){U[0] = 500; U[1] = 500;}
  if (i==1){U[0] = 500; U[1] = 1000;}
  if (i==2){U[0] = 1000; U[1] = 500;}
  if (i==3){U[0] = 1000; U[1] = 1000;}
  paritycheck();
}
//****************************************************************************
// SENDS THE IR DATA
BLYNK_WRITE(V6){
  int i=param.asInt();
  if (i==1){
  PrintTag();
  SendIR();
  }
}
//*****************************************************************************
void setup(){
  // Debug console
  Serial.begin(9600);
  // Set up Blynk Authentication
  Blynk.begin(auth, ssid, pass);
  // initialize the IR digital pin as an output:
  pinMode(IRledPin1, OUTPUT);
}
//******************************************************************************
void loop(){
  Blynk.run();
}
