// Set Bullet Type
BLYNK_WRITE(V0) {
int b=param.asInt();
if (b==1) {B[0]=500; B[1]=500; B[2]=500; B[5]=500;}
if (b==2) {B[0]=500; B[1]=500; B[2]=500; B[5]=1000;}
